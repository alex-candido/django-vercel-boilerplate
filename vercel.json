{
  "builds": [
    {
      "src": "src/django_app/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/django_app/wsgi.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
